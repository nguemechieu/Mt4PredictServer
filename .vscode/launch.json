{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "debugpy",
      "request": "launch",
      "name": "Launch mt4PredictServer",
      "program": "${workspaceFolder}/${input:programPath}",
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/.env"
    }
  ],
  "inputs": [
    {
      "type": "pickString",
      "id": "programPath",
      "description": "Select the Python file to debug",
      "options": [
        "app.py",
        "mt4PredictServer.py"
      ]
    }
  ]
}